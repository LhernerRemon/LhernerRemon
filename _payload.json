ype":26,"value":769},"Utilice mayúsculas o minúsculas según el tipo de objeto",{"type":21,"tag":328,"props":771,"children":772},{},[773,775],{"type":26,"value":774},"Esto da como resultado: ",{"type":21,"tag":32,"props":776,"children":777},{},[778],{"type":26,"value":779},"\"createdAt\": 1320296464",{"type":21,"tag":66,"props":781,"children":783},{"id":782},"autenticación",[784],{"type":26,"value":785},"Autenticación",{"type":21,"tag":22,"props":787,"children":788},{},[789,791,796,798,803],{"type":26,"value":790},"Las aplicaciones web o móviles que exponen API ",{"type":21,"tag":32,"props":792,"children":793},{},[794],{"type":26,"value":795},"no tienen que compartir contraseñas",{"type":26,"value":797},". Permite al proveedor de API ",{"type":21,"tag":32,"props":799,"children":800},{},[801],{"type":26,"value":802},"revocar tokens",{"type":26,"value":804}," para un usuario individual, para una aplicación completa, sin requerir que el usuario cambie su contraseña original.",{"type":21,"tag":22,"props":806,"children":807},{},[808],{"type":26,"value":809},"Esto es fundamental si un dispositivo móvil se ve comprometido o si se descubre una aplicación no autorizada.",{"type":21,"tag":66,"props":811,"children":813},{"id":812},"ssl",[814],{"type":26,"value":815},"SSL",{"type":21,"tag":22,"props":817,"children":818},{},[819],{"type":26,"value":820},"Va a haber intercambio de mucha información sensible, como los token. SSL es de uso obligatorio y es gratis.",{"type":21,"tag":66,"props":822,"children":824},{"id":823},"cache",[825],{"type":26,"value":826},"Cache",{"type":21,"tag":22,"props":828,"children":829},{},[830,832,837],{"type":26,"value":831},"Una compensación fundamental en los sitios web dinámicos es que son dinámicos. Almacenar en caché algo es guardar el resultado de un ",{"type":21,"tag":32,"props":833,"children":834},{},[835],{"type":26,"value":836},"cálculo costoso",{"type":26,"value":838}," para que no tenga que realizar el cálculo la próxima vez.",{"type":21,"tag":22,"props":840,"children":841},{},[842,844,849],{"type":26,"value":843},"No vayas a realizar ",{"type":21,"tag":32,"props":845,"children":846},{},[847],{"type":26,"value":848},"las mismas consultas otra vez",{"type":26,"value":57},{"type":21,"tag":66,"props":851,"children":853},{"id":852},"limita-las-solicitudes",[854],{"type":26,"value":855},"Limita las solicitudes",{"type":21,"tag":22,"props":857,"children":858},{},[859],{"type":26,"value":860},"La limitación es similar a los permisos, ya que determina si se debe autorizar una solicitud. Utiliza ‘limitadores’ para controlar la tasa de solicitudes que los clientes pueden realizar a tu API.",{"type":21,"tag":862,"props":863,"children":865},"h5",{"id":864},"fuente-apigee",[866,868],{"type":26,"value":867},"Fuente: ",{"type":21,"tag":869,"props":870,"children":874},"a",{"href":871,"rel":872},"https://pages.apigee.com",[873],"nofollow",[875],{"type":26,"value":876},"Apigee",{"title":431,"searchDepth":878,"depth":878,"links":879},2,[880,881,882,883,884],{"id":61,"depth":878,"text":64},{"id":297,"depth":878,"text":300},{"id":495,"depth":878,"text":498},{"id":570,"depth":878,"text":573},{"id":693,"depth":878,"text":696},"markdown","content:blog:good-practices-desing-web-api.md","content","blog/good-practices-desing-web-api.md","md",1699149820703]